{
  "name": "food-selector",
  "version": "1.0.0",
  "description": "Polls users on their food preferences",
  "main": "index.js",
  "scripts": {
    "start": "node dist_node/index.js",
    "dev": "nodemon node_ts/index.ts",
    "test": "jest",
    "build": "npx babel node_ts --out-dir dist_node --extensions .ts,.tsx && webpack --config ./webpack.config.js --mode production",
    "webpackDev": "webpack --config ./webpack.config.js --mode development --watch",
    "lint": "eslint src/*.js src/*.ts src/components/*.tsx node_ts/*.ts node_ts/routes/api/*.ts --fix",
    "check-types": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajohnson25/food-selector.git"
  },
  "author": "Adam Johnson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajohnson25/food-selector/issues"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "> 0.25%",
              "not dead"
            ]
          },
          "corejs": "3.6.5",
          "useBuiltIns": "usage"
        }
      ],
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "homepage": "https://github.com/ajohnson25/food-selector#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/express": "^4.17.7",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/materialize-css": "^1.0.8",
    "@types/node": "^14.0.24",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.12.21",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^26.1.3",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "eslint-plugin-jest": "^23.18.0",
    "express": "^4.17.1",
    "express-promise-router": "^3.0.3",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-materialize": "^3.9.2",
    "regenerator-runtime": "^0.13.5",
    "ts-node": "^8.10.2",
    "uuid": "^8.2.0"
  }
}
